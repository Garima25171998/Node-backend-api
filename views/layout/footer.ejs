<!------------------------------------------------- User Profile model ------------------------------->
<div class="modal fade" id="user-profile" role="dialog">
	<div class="modal-dialog r-2x">
		<div class="modal-content r-2x b-b-3x b-b-theme">
			<div class="modal-header bg-info">
				<button type="button" class="close" data-dismiss="modal"><i class="fa fa-times text-black m-t-xs"></i></button>
				<h4 class="modal-title text-white font-bold">User Profile</i><span></span></h4>
			</div>
			<div class="modal-body">
					<div class="row">
						<div class="col-md-6">
							<div class="form-group">
								<label class="control-label font-weight-600">User Name</label>
								<input type="text" class="form-control input-sm user-profile-validation" name="username">
							</div>
						</div>
						<div class="col-md-6">
							<div class="form-group">
								<label class="control-label font-weight-600">Company <span class="text-10">[printed on receipt]</span></label>
								<input type="text" class="form-control input-sm user-profile-validation" name="company">
							</div>
						</div>
						<div class="col-md-6">
							<div class="form-group">
								<label class="control-label font-weight-600">Address <span class="text-10">[printed on receipt]</span></label>
								<input type="text" class="form-control input-sm user-profile-validation" name="c_address">
							</div>
						</div>
						<div class="col-md-6">
							<div class="form-group">
								<label class="control-label font-weight-600">Location Category <span class="text-10">[showroom etc..]</span></label>
								<input type="text" class="form-control input-sm user-profile-validation" name="c_location">
							</div>
						</div>
						<div class="col-md-6">
							<div class="form-group">
								<label class="control-label font-weight-600">Contact No <span class="text-10">[printed on receipt]</span></label>
								<input type="text" class="form-control input-sm user-profile-validation" name="c_contact">
							</div>
						</div>
						<div class="col-md-12">
							<button type="submit" class="btn btn-3d m-b-xs w-xs btn-sm btn-success user-profile-submit"> Save</button>
							<button type="button" class="btn btn-3d m-b-xs  btn-sm btn-dark" data-dismiss="modal">Cancel</button>
						</div>
					</div>
			</div>
		</div>

	</div>
</div>
<!-------------------------------------------------End User Profile model ------------------------------->


<!------------------------------------------------- User Change password model ------------------------------->
<div class="modal fade" id="user-changepassword" role="dialog">
	<div class="modal-dialog r-2x">
		<div class="modal-content r-2x b-b-3x b-b-theme">
			<div class="modal-header bg-info">
				<button type="button" class="close" data-dismiss="modal"><i class="fa fa-times text-black m-t-xs"></i></button>
				<h4 class="modal-title text-white font-bold">Change password</i><span></span></h4>
			</div>
			<div class="modal-body">
					<div class="row">
						<div class="col-md-12">
							<div class="form-group">
								<label class="control-label font-weight-600">Old Password</label>
								<input type="hidden" name="pass"/>
								<input type="text" class="form-control input-sm user-profile-validation" name="oldpass">
							</div>
						</div>
						<div class="col-md-12">
							<div class="form-group">
								<label class="control-label font-weight-600">New Password</label>
								<input type="text" class="form-control input-sm user-profile-validation" name="newpass" onblur="checklength(this)">
							</div>
						</div>
						<div class="col-md-12">
							<div class="form-group">
								<label class="control-label font-weight-600">Confirm Password</label>
								<input type="text" class="form-control input-sm user-profile-validation" name="confirmpass">
							</div>
						</div>
						
						<div class="col-md-12">
							<button type="submit" class="btn btn-3d m-b-xs w-xs btn-sm btn-success user-submit-password"> Save</button>
							<button type="button" class="btn btn-3d m-b-xs  btn-sm btn-dark" data-dismiss="modal">Cancel</button>
						</div>
					</div>
			</div>
		</div>

	</div>
</div>
<!-------------------------------------------------End User Change password model ------------------------------->

